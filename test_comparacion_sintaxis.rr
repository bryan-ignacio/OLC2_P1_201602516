System.out.println("=== COMPARACION DE SINTAXIS DE FUNCIONES ===");

// Sintaxis 1: Con palabra clave 'func' (sintaxis original)
int func funcionConFunc(){
    return 100;
};

// Sintaxis 2: Sin palabra clave 'func' (nueva sintaxis)
int funcionSinFunc(){
    return 200;
};

// Ambas sintaxis también funcionan con parámetros
int func sumaConFunc(int a, int b){
    return a + b;
};

int sumaSinFunc(int a, int b){
    return a + b;
};

System.out.println("=== PROBANDO AMBAS SINTAXIS ===");

// Probando funciones sin parámetros
int resultado1 = funcionConFunc();
System.out.println("funcionConFunc() = ");
System.out.println(resultado1);

int resultado2 = funcionSinFunc();
System.out.println("funcionSinFunc() = ");
System.out.println(resultado2);

// Probando funciones con parámetros
int resultado3 = sumaConFunc(10, 20);
System.out.println("sumaConFunc(10, 20) = ");
System.out.println(resultado3);

int resultado4 = sumaSinFunc(15, 25);
System.out.println("sumaSinFunc(15, 25) = ");
System.out.println(resultado4);

System.out.println("=== AMBAS SINTAXIS FUNCIONAN CORRECTAMENTE ===");