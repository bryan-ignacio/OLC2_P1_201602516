void main() {
    int x = 5;
    int y = 10;
    int resultado = x + y;
    println("El resultado es: " + resultado);
    
    int[] numeros = new int[3];
    numeros[0] = 1;
    numeros[1] = 2;
    numeros[2] = 3;
    
    for (int i = 0; i < numeros.length; i++) {
        println("Numero " + i + ": " + numeros[i]);
    }
}